http://bestiar.drdplus.loc:2015 {
    tls mail@jaroslavtyc.com
    gzip
    fastcgi / 127.0.0.1:9000 php {
        ext   .php
        split .php
        index index.php
    }
    root /var/www/bestiar
    log /var/log/caddy
}

